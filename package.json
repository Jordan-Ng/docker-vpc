{
  "name": "docker-vpc",
  "version": "1.0.0",
  "description": "Mac GUI with UI identical to AWS console for creating, managing and orchestrating cloud services with docker.",
  "main": "main.js",
  "scripts": {
    "start": "npm run build \n electron .",
    "build": "webpack --mode production",
    "dev": "concurrently \"npm run react:serve\" \"npm run electron:run \"",
    "react:serve": "webpack serve --config webpack.config.js --port 3000",
    "electron:run": "wait-on http://localhost:3000 && electron ."
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@mantine/core": "^7.11.2",
    "@mantine/hooks": "^7.11.2",
    "@tabler/icons-react": "^3.11.0",
    "electron": "^31.2.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.25.1",
    "sass": "^1.77.8",
    "style-loader": "^4.0.0",
    "webpack-cli": "^5.1.4"
  },
  "devDependencies": {
    "@babel/core": "^7.24.9",
    "@babel/preset-env": "^7.24.8",
    "@babel/preset-react": "^7.24.7",
    "babel-loader": "^9.1.3",
    "concurrently": "^8.2.2",
    "css-loader": "^7.1.2",
    "html-webpack-plugin": "^5.6.0",
    "postcss": "^8.4.39",
    "postcss-preset-mantine": "^1.16.0",
    "postcss-simple-vars": "^7.0.1",
    "sass-loader": "^14.2.1",
    "wait-on": "^7.2.0",
    "webpack": "^5.93.0",
    "webpack-dev-server": "^5.0.4"
  }
}
